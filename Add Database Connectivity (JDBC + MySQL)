CREATE DATABASE vpn_db;

USE vpn_db;

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(50) NOT NULL,
    password VARCHAR(100) NOT NULL,
    role VARCHAR(20) NOT NULL
);
INSERT INTO users (username, password, role) VALUES 
